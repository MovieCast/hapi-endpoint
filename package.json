  "bugs": {
    "url": "https://github.com/MovieCast/hapi-endpoint/issues"
  },
  "homepage": "https://github.com/MovieCast/hapi-endpoint#readme",
  "dependencies": {
    "boom": "^7.2.0",
    "glob": "^7.1.2",
    "hoek": "^5.0.3",
    "joi": "^13.5.2"
  },
  "devDependencies": {
    "code": "^5.2.0",
    "coveralls": "^3.0.2",
    "debug": "^3.1.0",
    "hapi": "^17.5.2",
    "lab": "^15.5.0"
  }
